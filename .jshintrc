{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "unused": "vars",
    "strict": true,
    "trailing": true,
    "maxparams": 3,
    "node": true,
    "overrides": {
      "test/**": {
        "mocha": true
      },
      "lib/browser/client-scripts/*": {
        "node": false,
        "browserify": true
      }
    }
}
